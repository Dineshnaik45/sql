 CREATE TABLE STUDENT(ID NUMBER(4) NOT NULL,NAME VARCHAR(20) UNIQUE ,
 CITY VARCHAR(10) DEFAULT('JAMNER'),
 MARKS NUMBER(6) CHECK(MARKS>=35),
 CID NUMBER(8) PRIMARY KEY
 );
/* HERE I CREATE STUDENT TABLE AND APLLEID ALL CONSTRAINR AND 
ALSO ADDED PRIMARY KEY FROR ESTABLISH RELATION BETWEEN TWO TABLE */

/* INSERT OPERATION */
 INSERT INTO STUDENT VALUES(2,'DINESH','PUNE',40,10);
/* HERE I INSERTED BOTH COLUMN BY VALUES */
 INSERT INTO STUDENT (ID,NAME,MARKS,CID) VALUES(2,'NAIK',35,20);
/* HERE I FORGOTE INSERT DATA IN CITY COLUMN BECAUSE OF CHECKING FOR DEFAULT VALUES WILL WORK OR NOT */
 
 /* THEN I CREATE NEW TABLE FOR FOREIGN KEY AND PRIMARY KEY RELATION*/
  CREATE TABLE STUDENT1 (SID NUMBER(20) REFERENCES STUDENT(CID),
ENAME VARCHAR(20) );
/* HERE I CREATE SID COLUMN WITH PRIMARY KEY AND FOREIGN KEY RELATION */
INSERT INTO STUDENT1 VALUES(20,'RAJ');
/* HERE I INSERT DATA FOR CHECKING PRIMARY KEY AND FOREIGN KEY RELATION */
INSERT INTO STUDENT1 VALUES(10,'RAM');

